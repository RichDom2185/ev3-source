#!/usr/bin/nodejs

require('/usr/local/lib/node_modules/ev3_source.js');

eval((function() {
    if (process.argv.length != 3) {
        console.error("Usage: source3 <file name>");
        process.exit(1);
    }

    var source_in = require('fs').readFileSync(process.argv[2], 'utf-8');
    // TODO: transpile
    return source_in;
})());
